<body data-new-gr-c-s-check-loaded="14.984.0" data-gr-ext-installed="">
  
          <!-- search bar -->
  <div id="search_bar">
    <%= form_tag(search_index_path, method: "get") do %>
    <%= text_field_tag :query %>
    <%= submit_tag("Search", :style => "color: white; font-family: \"Futura\";") %>
    <% end %>
  </div>
 


  <div class = "container" id = "narrow-search">
    <button class="btn btn-dark" data-toggle="collapse" data-target="#fundsNavbar" aria-expanded="false" aria-controls="fundsNavbar" role = "button">
      Narrow Search
    </button>
    <br>
    <div class="collapse" id="fundsNavbar">
      <%= render partial: 'layouts/funds_navbar' %>
    </div>
  </div>
  
    <main role="main">
      <div class = "album py-5 bg-light">
        <div class = "container">
          <div class = "row">
            <% @resources.each do |resource| %>
              <div class = "col-lg-3 col-md-4 col-sm-12", align = "center">
                <h2 style = "color:black"> <%= resource.title %></h2>
                <p><%= resource.description %><p>
                <p>
                  <a href = "https://www.grinnell.edu/user/sannings" class = "btn btn-primary">Contact Info</a>
                </p>
              </div>
            <% end %>
          </div>
        </div>
      </div>
    </main> 

