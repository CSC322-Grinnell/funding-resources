<div class="container">
<div class="row">
  <%= form_tag(search_index_path, method: "get") do %>
  <h2><%= label_tag(:query, "Search for:") %></h2>
  <%= text_field_tag(:query) %>
  <%= submit_tag("Search") %>
<% end %>
</div>
</div>

<% @resources.each do |resource| %>
<div class="container">

    <h1><%= link_to resource.title, resource_path(resource)%></h1>

</div>
<% end %>
